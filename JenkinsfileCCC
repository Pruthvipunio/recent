pipeline {
  agent any
  stages {
     stage ('BUILD') {
        step {
          echo "This is Build stage"
          sh 'sleep 5'
        }
     }

     stage ('TEST') {
        step {
          echo "This is Build stage"
          sh 'sleep 5'
        }
     }

     stage ('DEPLOY') {
        step {
          echo "This is Build stage"
          sh 'sleep 5'
        }
     }
  }
}  
