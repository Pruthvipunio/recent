pipeline {
  agent none
  stages {
    stage ('BUILD') {
      agent { label 'masterslave' }
      steps {
        echo "This is Build stage"
        sh 'sleep 5'
      }
    }
stage ('DEPLOY') {
      agent { label 'slave1' }
      steps {
        echo "This is Deploy stage"
        sh 'sleep 5'
      }
    }
stage ('TEST') {
      agent { label 'slave2' }
      steps {
        echo "This is Test stage"
        sh 'sleep 5'
      }
    }
  }

}
