pipeline {
  agent any
  stages {
    stage ('TEST') {
      parallel {
        stage (TEST 1) {
          steps {
             echo "this is test1 stage"}
             sh 'sleep 5'
        }
        stage (TEST 2) {
          steps {
             echo "this is test2 stage"}
             sh 'sleep 5'
        }
      }
    }
  }
}
