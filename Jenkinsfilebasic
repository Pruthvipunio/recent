pipeline {
  agent any;
  stages {
    stage ('BUILD') {
      steps {
        sh '''
        sleep 5
        echo "This is Build stage"
        '''
      }
    }
    
    stage ('TEST') {
      steps {
        sh '''
        sleep 5
        echo "This is Build stage"
        '''
      }
    }

    stage ('DEPLOY') {
      steps {
        sh '''
        sleep 5
        echo "This is Build stage"
        '''
      }
    } 
  }

}
