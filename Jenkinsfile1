pipeline {
 agent {
  label 'jenkins'
 }
   stages {
    stage ('BUILD') {
      steps {
       echo "this is new Build stage"
      }
    }

    stage ('DEPLOY') {
      steps {
       echo "this is new Build stage"
      }
    }
   prallel {
    stage ('TEST') {
      steps {
       echo "this is new Build stage"}
      }
    }

   stage ('TEST 1') {
      steps {
       echo "this is new Build stage"}
      }
    }

   stage ('TEST 2') {
      steps {
       echo "this is new Build stage"}
      }
    }
   


